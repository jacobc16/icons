{
	"name": "@steeze-ui/phosphor-icons",
	"description": "Icon Sources (based on template) built for @steeze-ui/icons",
	"version": "2.1.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "pnpm build:icons && svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"build:icons": "node build.js"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"devDependencies": {
		"@phosphor-icons/core": "^2.1.1",
		"@steeze-ui/icons": "workspace:^",
		"@steeze-ui/svelte-icon": "workspace:^",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.30.4",
		"@sveltejs/package": "^2.3.2",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"publint": "^0.1.16",
		"svelte": "^3.59.2",
		"svelte-check": "^3.8.4",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3",
		"vite": "^4.5.3"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/steeze-ui/icons.git"
	},
	"keywords": [
		"phosphor",
		"phosphor-icons",
		"raw",
		"icons",
		"steeze",
		"regular",
		"thin",
		"light",
		"bold",
		"fill",
		"duotone"
	],
	"author": "Justin Voitel <mail@justinv.me>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/steeze-ui/icons/issues"
	},
	"homepage": "https://github.com/steeze-ui/icons#readme",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	}
}